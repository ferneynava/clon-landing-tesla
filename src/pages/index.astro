---
import Accessories from '../components/Accessories.astro';
import HeroSection from '../components/HeroSection.astro';
import LandingHeader from '../components/LandingHeader.astro';
import Model3 from '../components/Model3.astro';
import ModelS from '../components/ModelS.astro';
import ModelX from '../components/ModelX.astro';
import ModelY from '../components/ModelY.astro';
import Powewall from '../components/Powewall.astro';
import Section from '../components/Section.astro';
import SolarPanels from '../components/SolarPanels.astro';
import SolarRoof from '../components/SolarRoof.astro';
import Layout from '../layouts/Layout.astro';
---

<Layout title="Tesla">
	<LandingHeader />
	
	<main class="snap-y snap-mandatory relative w-full h-screen overflow-auto overflow-x-hidden scroll-smooth">
		<div class="snap-center">
			<HeroSection/>
		</div>
		<div class="snap-center">
			<ModelS />
		</div>
		<div class="snap-center">
			<Model3 />
		</div>
		<div class="snap-center">
			<ModelX />
		</div>
		<div class="snap-center">
			<ModelY />
		</div>
		<div class="snap-center">
			<SolarRoof />
		</div>
		<div class="snap-center">
			<SolarPanels />
		</div>
		<div class="snap-center">
			<Powewall />
		</div>
		<div class="snap-center">
			<Accessories />
		</div>
	</main>
</Layout>

